config_version=5

[application]

config/name="simplesound"
config/description="a simple music player built to be like spotify but uses music on your local machine"
config/version="v1.01-alpha"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.6", "C#", "GL Compatibility")
run/max_fps=60
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/stretch_mode=0
boot_splash/use_filter=false
config/icon="res://Icons/logotrans.png"
config/windows_native_icon="res://Icons/mainicon.ico"

[audio]

buses/default_bus_layout="uid://c34ugugid1knj"

[display]

window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Simplesound"

